.navbar {
	position:absolute;
	left: 0px;
	width:90px;
	height:100vh;
	background-color:#F1C97D;
	display: flex;
	flex-direction: column;
}
.navbar a {
	height:33%;
    display: flex;
    justify-content: center;
    align-items: center;
    padding:20px;
}

}
.navbar svg {
	text-align:center;
	
}



#paperPlaneSVG{
	stroke-miterlimit:10; 
	stroke:#000000; 
	stroke-width:2;
	stroke-linecap:round;
	stroke-linejoin:round;
}

.userSVG{
	stroke:#000000; 
	stroke-width:2;
	stroke-linecap:round;
	stroke-linejoin:round;
}

.icon{
	width:96px;
	height:96px;
	viewBox:0 0 96 96;
	fill:black;
}

.navbar a:hover {
	background-color: white;
}